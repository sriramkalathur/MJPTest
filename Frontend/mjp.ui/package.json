  "private": true,
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/lab": "^5.0.0-alpha.153",
    "@types/react": "^18.0.2",
    "@types/react-router-dom": "^5.3.0",
    "axios": "^1.3.5",
    "bootstrap": "^5.2.3",
    "common": "file:../common",
    "env-cmd": "^10.1.0",
    "formik": "^2.2.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.8.0",
    "react-popper": "^2.3.0",
    "react-router": "^5.3.4",
    "react-router-dom": "^5.3.0",
    "react-scripts": "^4.0.3",
    "reactstrap": "^9.1.9",
    "watch": "^1.0.2",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "serve": "webpack serve",
    "start:dev": "env-cmd -f config.env.dev react-app-rewired --openssl-legacy-provider start --watch ./node_modules/common/src",
