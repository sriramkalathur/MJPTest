    "build:prod": "env-cmd -f config.env.prod react-app-rewired --openssl-legacy-provider build",
    "build:dev": "env-cmd -f config.env.dev react-app-rewired --openssl-legacy-provider build",
    "watch": "watch 'npm run start:dev' ./node_modules/src'",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "buildApp": "webpack"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react-dom": "^18.0.11",
    "@types/react-router": "^5.1.16",
    "customize-cra": "^1.0.0",
    "react-app-rewired": "^2.1.8"
